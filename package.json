{
  "name": "dotcms-js",
  "version": "4.2.0-b70",
  "license": "GPL 3",
  "author": "dotCMS",
  "main": "bundles/amazing.umd.js",
  "module": "dotcms-js.js",
  "typings": "index.d.ts",
  "description": "DotCMS UI Angular App",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotCMS/dotcms-js"
  },
  "bundleDependencies": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "transpile:dotcms-js": "ngc -p ./tsconfig.dotcms-js.json",
    "package:dotcms-js": "rollup -c",
    "minify:dotcms-js": "uglifyjs build-dotcms-js/bundles/amazing.umd.js --screw-ie8 --compress --mangle --comments --output build-dotcms-js/bundles/amazing.umd.min.js",
    "build:dotcms-js": "npm run transpile:dotcms-js && npm run package:dotcms-js && npm run minify:dotcms-js && npm run copy-files:dotcms-js",
    "copy-files:dotcms-js": "cd src && copyfiles './**/*.html' './**/*.css' './**/*.jpg' './**/*.gif' './**/*.png' ../build-dotcms-js/ && cd /"
  },
  "private": false,
  "dependencies": {
    "@angular/platform-server": "^6.1.0",
    "lodash": "4.16.6",
    "rxjs-compat": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/animations": "^6.1.0",
    "@angular/cli": "^6.1.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.85",
    "@types/uglify-js": "^2.6.28",
    "codelyzer": "~4.2.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-cli": "^0.7.0",
    "primeng": "^4.0.1",
    "protractor": "~5.1.2",
    "rollup": "^0.47.4",
    "rxjs": "^6.2.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "2.9.2",
    "zone.js": "^0.8.26"
  }
}
